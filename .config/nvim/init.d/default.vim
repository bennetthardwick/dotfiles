" -- Vim Config -- "

if &compatible
 set nocompatible
endif

set number
set relativenumber
set expandtab
set mouse=a
set tabstop=2
set expandtab
set shiftwidth=2
set autoindent
set smartindent
set updatetime=100
set noshowmode
set clipboard+=unnamedplus

set wildignore+=*/node_modules/**/*
set wildignore+=*.aux

syntax on
