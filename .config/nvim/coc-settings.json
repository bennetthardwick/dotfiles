{
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "tsserver.log": "verbose",

  "python.formatting.provider": "yapf",
  "python.autoComplete.extraPaths": [
    "/opt/google-cloud-sdk/platform/google_appengine/",
    "/opt/google-cloud-sdk/platform/google_appengine/lib/webapp2-2.5.2/",
    "/home/bennett/clipchamp/clipchamp-stack/backend/modules/v2/",
    "/home/bennett/clipchamp/clipchamp-stack/backend/modules/v2/v2/"
  ],
  "python.jediEnabled": false,
  "python.autoUpdateLanguageServer": true,
  "python.analysis.downloadChannel": "daily",

  "eslint.trace.server": "verbose",

  "rust-analyzer.cargo.features": "all",
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.checkOnSave.allTargets": true,
  "rust-analyzer.cargo.unsetTest": [],

  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache"
        }
      }
    }
  }
}
